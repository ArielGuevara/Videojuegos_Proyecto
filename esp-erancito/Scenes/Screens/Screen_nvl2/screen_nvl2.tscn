[node name="ItemHistoria4" parent="." instance=ExtResource("8_1e86a")]
position = Vector2(5868, 405)
frase_del_juego = "A veces la vida es una fila india, 
pero otras veces es como lavar platos: 
el ultimo plato que pones encima es el primero 
que tienes que lavar. Aprende a identificar en que orden debes atender 
las cosas para no bloquearte."

[node name="ItemHistoria5" parent="." instance=ExtResource("8_1e86a")]
